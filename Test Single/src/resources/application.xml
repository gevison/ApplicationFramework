<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="TestSingleApplication" class="ge.framework.test.application.single.TestSingleFrameApplication" lazy-init="true">
        <property name="applicationRestarterClass" value="ge.framework.application.core.utils.JavaApplicationRestarter"/>
        <property name="allowMultipleApplications" value="false"/>
        <property name="configurationClass" value="ge.framework.test.application.single.objects.TestSingleApplicationConfiguration"/>
        <property name="configurationName" value="TestSingle.xml"/>
        <property name="frameClass" value="ge.framework.test.frame.single.TestSingleFrameApplicationFrame"/>
        <property name="largeIcon" value="Application.ico:2"/>
        <property name="largeImage" value="Application.ico:2"/>
        <property name="smallIcon" value="Application.ico:4"/>
        <property name="smallImage" value="Application.ico:4"/>
        <property name="macIcon" value="Application.ico:1"/>
        <property name="macImage" value="Application.ico:1"/>
        <property name="metaDataName" value=".TestSingle"/>
        <property name="name" value="Test Single Application"/>
        <property name="description" value="Test Single Application Description"/>
    </bean>
</beans>