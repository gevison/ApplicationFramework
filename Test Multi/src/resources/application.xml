<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="TestMultiFrameApplication" class="ge.framework.test.application.multi.TestMultiFrameApplication">
        <property name="allowMultipleApplications" value="false"/>
        <property name="configurationClass"
                  value="ge.framework.test.application.multi.objects.TestMultiApplicationConfiguration"/>
        <property name="configurationName" value="TestMultiApp.xml"/>
        <property name="largeIcon" value="Application.ico:2"/>
        <property name="largeImage" value="Application.ico:2"/>
        <property name="smallIcon" value="Application.ico:4"/>
        <property name="smallImage" value="Application.ico:4"/>
        <property name="macIcon" value="Application.ico:1"/>
        <property name="macImage" value="Application.ico:1"/>
        <property name="metaDataName" value=".TestMultiApp"/>
        <property name="name" value="Test Multi Application"/>
        <property name="description" value="Test Multi Application Desciption"/>
        <property name="frameName" value="Test Multi Frame"/>
        <property name="frameClass" value="ge.framework.test.frame.single.TestMultiFrameApplicationFrame"/>
        <property name="frameConfigurationClass" value="ge.framework.test.frame.single.objects.TestMultiFrameConfiguration"/>
        <property name="frameMetaDataName" value=".TestMulti"/>
        <property name="frameConfigurationName" value="TestMulti.xml"/>
    </bean>
</beans>